procedure Main() is
begin
    let variable: Int32 = 0;

    match variable
        0 => Print_Line("Zero!"),
        1 => Print_Line("One!"),
        2 => Print_Line("Two!"),
        _ => Print_Line("Uhhhh idk... :/"),
    end match;

    if variable == 0 then
        Print_Line("Zero!");
    elsif variable == 1 then
        Print_Line("One!");
    elsif variable == 2 then
        Print_Line("Two!");
    else
        Print_Line("Uhhhh idk... :/");
    end if;
end Main;
