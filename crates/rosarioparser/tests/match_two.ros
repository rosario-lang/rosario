use mada::text_io::*;

procedure Main() is
begin
    let variable: Int32 = 0;

    let result: Int32 = match variable
        0 => begin
            Print_Line("Hello from Zero!");
            0
        end,
        _ => begin
            Print_Line("Hello from The Rest of the Numbers!");
            1
        end,
    end result;
end Main;
